@model Rupor.Public.Models.Profile.InfoViewModel
@{
    ViewBag.Title = "Info";
}

@Html.Action("UserInfo")
<div class="col-md-5">
    <form action="@Url.Action("Info")" method="post" class="form-horizontal">
        @Html.AntiForgeryToken()
        @Html.HiddenFor(m => m.Id)
        @Html.HiddenFor(m => m.Email)
        @Html.HiddenFor(m => m.OriginalPictureId)
        @Html.HiddenFor(m => m.MinPictureId)
        @Html.ValidationSummary()
        <div class="form-group">
            @Html.TextBoxFor(m => m.GivenName, new { @class = "form-control", placeholder = "Укажите имя" })
        </div>
        <div class="form-group">
            @Html.TextBoxFor(m => m.FamilyName, new { @class = "form-control", placeholder = "Укажите фамилию" })
        </div>
        <div class="form-group">
            @Html.TextBoxFor(m => m.BirthDate, new { @class = "form-control", placeholder = "vk ссылка на профиль" })
        </div>
        <div class="form-group">
            @Html.TextBoxFor(m => m.SocialLinkVk, new { @class = "form-control", placeholder = "vk ссылка на профиль" })
        </div>
        <div class="form-group">
            @Html.TextBoxFor(m => m.SocialLinkFb, new { @class = "form-control", placeholder = "fb ссылка на профиль" })
        </div>
        <div class="form-group">
            @Html.TextBoxFor(m => m.SocialLinkInst, new { @class = "form-control", placeholder = "instagramm ссылка на профиль" })
        </div>
        <div class="form-group">
            @Html.TextBoxFor(m => m.SocialLinkTwitter, new { @class = "form-control", placeholder = "twitter ссылка на профиль" })
        </div>
        <button class="btn btn-primary">Редактировать</button>
    </form>
</div>






